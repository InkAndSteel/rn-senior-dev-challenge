{
  "expo": {
    "name": "rn-senior-dev-challenge",
    "slug": "rn-senior-dev-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "rnseniordevchallenge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}
